                                <thead>
                                    <tr>
                                        <th>Codigo de Proyecto</th>
                                        <th>Evaluacion</th>
                                        <th>Calificativo </th>
                                        <th>Porcentaje</th>

                                    </tr>
                                </thead>

                                <tbody>
                                    @foreach (var file in Model.SP_WT_DETALLE_RESULTADOS_AUDITORIA_2)
                                    {
                                        <tr>
                                            <td>
                                                @file.Proyecto
                                            </td>
                                            <td>
                                                @file.Evaluacion
                                            </td>
                                            <td>
                                                @file.Calificativo
                                            </td>
                                            <td>
                                                @file.Porcentaje
                                            </td>

                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <br />


    </div>

    <br />




</body>
