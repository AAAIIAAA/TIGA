@using WebTIGA;
@model WebTIGA.Models.ContenedorModelos

<html lang="en">
<head>
   
    <script type="text/javascript" class="init">
        $(document).ready(function () {
            $('#example1').DataTable({
                dom: 'Bfrtip',
                "pageLength": 15,
                buttons: [
                    'excelHtml5',
                ]
            });
        });
    </script>

</head>
<body>
    <div class="card shadow mb-2">
        @*<div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-info">DETALLE DE CUENTAS SIGNIFICATIVAS</h6>
        </div>*@
        <div class="card-body">
            @*<div class="card-body">*@
                <div class="table-responsive">
                    <table id="example1" class="display nowrap" width="100%" cellspacing="0">
                        <thead>
                            <tr>
                                <th style="text-align:center">Cuenta Contable</th>
                                <th style="text-align:center">Descripción</th>
                                <th style="text-align:center">Saldo Final Local</th>
                                <th style="text-align:center">Movimiento Local</th>
                                <th style="text-align:center">Significativa Formulada?</th>
                                <th style="text-align:center">Significativa?</th>
                                <th style="text-align:center">Cobertura</th>
                                <th style="text-align:center">Macroproceso</th>
                                <th style="text-align:center">Evaluación</th>
                                <th style="text-align:center">Código</th>
                            </tr>
                        </thead>

                        <tbody>
                            @foreach (var item in Model.SP_BC_CUENTAS_SIGNIFICATIVAS)
                            {
                            <tr>
                                <td style="text-align:initial"> @item.CUENTA_CONTABLE </td>
                                <td style="text-align:initial"> @item.DESCRIPCION </td>
                                <td style="text-align:initial"> @item.SALDO_FINAL_LOCAL </td>
                                <td style="text-align:initial"> @item.MOVIMIENTO_LOCAL </td>
                                <td style="text-align:initial"> @item.CUENTA_SIGNIFICATIVA_FORMULADA </td>
                                <td style="text-align:initial"> @item.CUENTAS_SIGNIFICATIVAS </td>
                                <td style="text-align:initial"> @item.COBERTURA </td>
                                <td style="text-align:initial"> @item.Macroproceso </td>
                                <td style="text-align:initial"> @item.Evaluación </td>
                                <td style="text-align:initial"> @item.Codigo </td>
                            </tr>
                            }
                        </tbody>
               
                    </table>
                </div>
            @*</div>*@
        </div>
    </div>
</body>
</html>



