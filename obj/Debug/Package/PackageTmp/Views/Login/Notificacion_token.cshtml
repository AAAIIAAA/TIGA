@model WebTIGA.Models.RecoveryViewModel

@{
    ViewBag.Title = "Notificacion_token";
    Layout = "~/Views/Shared/_Layout_Forgot_Password.cshtml";
}

    <html>
    <head>
        <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
        <link rel="stylesheet" href="https://maxcdn.bootstr apcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
        <script src="https://unpkg.com/gijgo@1.9.13/js/gijgo.min.js" type="text/javascript"></script>
        <link href="https://unpkg.com/gijgo@1.9.13/css/gijgo.min.css" rel="stylesheet" type="text/css" />
        <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.0/css/all.css" integrity="sha384-lZN37f5QGtY3VHgisS14W3ExzMWZxybE1SJSEsQp9S+oqd12jhcu+A56Ebc1zFSJ" crossorigin="anonymous">
    </head>

    <body>


        <div class="row justify-content-center">

            <div class="col-xl-10 col-lg-12 col-md-9">
                <br /> <br /> <br /><br /><br /><br /><br />
                <div class="card o-hidden border-0 shadow-lg my-5">
                    <div class="card-body p-0">

                        <div class="row">
                            <div class="bg-gradient-info col-lg-6 d-none d-lg-block align-content-xl-between " style="background-image: url('../img/logos/L2.png')"></div>
                            <div class="col-lg-6">
                                <div class="p-5">
                                    <div align="center"> <img src="~/img/logos/ALERTA_F.png" width="200" height="200" class=" text-center" alt="Cinque Terre"></div>
                                    <br /><br />
                                    <div class="text-center">
                                        <h2 class="h4 text-gray-900 mb-2">Tu Solicitude de restableciento de contraseña ha expirado.</h2>
                                        <p class="mb-4"></p>
                                    </div>
                                    <hr>
                                    <div class="text-center">
                                        @Html.ActionLink("Iniciar Sesión", "Login_", new { @style = "color:a" })
                                    </div>
                                    <style>
                                        a {
                                            color: #1f99a4;
                                            text-decoration: none;
                                        }


                                            a:hover {
                                                color: #63c6c6;
                                            }
                                    </style>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>

            </div>

        </div>



        <script type="text/javascript">
            function disableButton() {
                var btn = document.getElementById('btnUpload');
                btn.disabled = true;
                btn.innerText = 'Registrando...'

                var btn2 = document.getElementById('btnReturn');
                btn2.disabled = true;

            }
        </script>
    </body>
</html>